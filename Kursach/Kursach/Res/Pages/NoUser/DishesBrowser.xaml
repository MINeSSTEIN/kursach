<Page x:Class="Kursach.Res.Pages.NoUser.DishesBrowser"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kursach.Res.Pages.NoUser"
      mc:Ignorable="d" 
      d:DesignHeight="150" d:DesignWidth="256"
      Title="DishesBrowser">

    <Grid>
        <Label Content="*Name*" x:Name="lName"/>
        <Label Content="*Price*" x:Name="lPrice" Margin="0,20,0,0"/>
        <Label Content="*Weight*" x:Name="lWeight" Margin="0, 40, 0, 0"/>
    </Grid>
</Page>
